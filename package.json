{
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epreston/enginelayer.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "browserslist": [
    "baseline 2025"
  ],
  "prettier": "@epreston/prettier-config",
  "packageManager": "npm@11.10.1",
  "engines": {
    "node": "^22.22.0 || >=24.13.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "lint": "eslint .",
    "format": "prettier --check --cache \"**/*.js\"",
    "format:fix": "prettier --write --cache \"**/*.js\""
  },
  "devDependencies": {
    "@epreston/eslint-config": "^2.3.0",
    "@epreston/prettier-config": "^2.1.0",
    "@epreston/tsconfig": "^1.2.1",
    "eslint": "^10.0.1",
    "prettier": "^3.8.1",
    "vite": "^7.3.1"
  }
}
